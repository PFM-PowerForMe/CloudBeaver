    "!lib/**/*.test.d.ts",
    "!lib/**/*.test.d.ts.map",
    "!lib/**/*.test.js.map",
    "!lib/tests",
    "!.tsbuildinfo"
  ],
  "packageManager": "yarn@4.9.2",
  "devDependencies": {
    "@dbeaver/tsconfig": "workspace:^",
    "rimraf": "^6",
    "typescript": "^5"
  }
}
